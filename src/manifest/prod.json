{
  "content_scripts": [
    {
      "matches": ["http://localhost:*/*"],
      "js": ["content-script.js"]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 3,
  "name": "click-to-react-component",
  "permissions": ["tabs"],
  "version": "1.0.0",
  "web_accessible_resources": [
    {
      "matches": ["http://localhost:*/*"],
      "resources": ["script.js"]
    }
  ]
}
