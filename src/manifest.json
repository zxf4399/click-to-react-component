{
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["http://localhost/*"],
      "js": ["content-script.js"]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 3,
  "permissions": ["tabs"],
  "web_accessible_resources": [
    {
      "matches": ["http://localhost/*"],
      "resources": ["script.js"]
    }
  ]
}
